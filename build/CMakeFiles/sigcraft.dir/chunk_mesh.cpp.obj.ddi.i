static BlockData access_safe(const ChunkData *chunk, ChunkNeighbors &neighbours, int x, int y, int z)
{

    if (y < 0 || y >= 384)
    {
        return BlockAir;
    }


    int chunk_dx = (x < 0) ? 0 : (x >= 16 ? 2 : 1);
    int chunk_dz = (z < 0) ? 0 : (z >= 16 ? 2 : 1);

    const ChunkData *target_chunk = neighbours.neighbours[chunk_dx][chunk_dz];
    if (target_chunk)
    {

        return chunk_get_block_data(target_chunk, x & (16 - 1), y, z & (16 - 1));
    }

    return BlockAir;
}




