void chunk_mesh(const ChunkData *chunk, ChunkNeighbors &neighbours, std::vector<ChunkMesh::Meshlet> &meshlets, size_t *num_verts)
{
    std::unordered_map<std::tuple<int, int, int>, int> index_map;
    int vertex_count = 0;
    int triangle_index = 0;
    int triangle_vertex_index = 0;
    int total_verts = 0;

    process_slice(0, 1, 2, chunk, neighbours, meshlets, index_map, vertex_count, triangle_index, triangle_vertex_index, total_verts);
    process_slice(1, 0, 2, chunk, neighbours, meshlets, index_map, vertex_count, triangle_index, triangle_vertex_index, total_verts);
    process_slice(2, 1, 0, chunk, neighbours, meshlets, index_map, vertex_count, triangle_index, triangle_vertex_index, total_verts);


    meshlets.back().num_verts = vertex_count;
    meshlets.back().num_tris = triangle_index;
    total_verts += vertex_count;
    *num_verts = total_verts;
}
